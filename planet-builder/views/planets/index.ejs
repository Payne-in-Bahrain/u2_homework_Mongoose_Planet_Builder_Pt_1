<%- include('../partials/header') %>

<h2>Planets List</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Climate</th>
      <th>Population</th>
    </tr>
  </thead>
  <tbody>
    <%planets.forEach((p)=>{%>
      <tr>
        <td><%= p.name %></td>
        <td><%= p.climate %></td>
        <td><%= p.population %></td>
      </tr>
      <%})%>
    <!-- Close the forEach using EJS here -->
  </tbody>
</table>

<%- include('../partials/footer') %>


<%- include('../partials/header') %>

<h2>Planet List</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Climate</th>
      <th>Population</th>
    </tr>
  </thead>
  <tbody>
    <% planets.forEach(planet => { %>
      <tr>
        <td><%= planet.name %></td>
        <td><%= planet.climate %></td>
        <td><%= planet.population %></td>
        </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>
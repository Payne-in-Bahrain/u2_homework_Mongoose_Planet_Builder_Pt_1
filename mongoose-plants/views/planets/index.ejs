<%- include('../partials/header') %>

<h2>Planet List</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Climate</th>
      <th>Population</th>
    </tr>
  </thead>
  <tbody>
    <% planets.forEach ((planet) => { %> <!-- Write the line of EJS to iterate over movies using forEach -->
      <tr>
        <td><%= planet.name %></td>
        <td><%= planet.climate %></td>
        <td><%= planet.population %></td>
      </tr>
    <% }) %> <!-- Close the forEach using EJS here -->
  </tbody>
</table>

<%- include('../partials/footer') %>